{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MultiUploadRequest",
  "type": "object",
  "required": ["metadata_tsv", "files"],
  "properties": {
    "metadata_tsv": {
      "type": "string",
      "format": "binary"
    },
    "files": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "binary"
      }
    }
  }
}
