{
  "$id": "https://sofi-platform.dk/user-info.v1.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "UserInfo",
  "type": "object",
  "properties": {
    "userId": {
      "type": "string"
    },
    "data_clearance": {
      "$ref": "./data-clearance.v1.json"
    },
    "institution": {
      "$ref": "./organization.v1.json"
    },
    "groups": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "permissions": {
      "type": "array",
      "items": {
        "$ref": "./permission.v1.json"
      }
    }
  }
}
